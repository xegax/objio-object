{
  "name": "objio-object",
  "version": "0.0.3",
  "description": "",
  "main": "index.js",
  "repository": "git@github.com:xegax/objio-object.git",
  "scripts": {
    "prepare": "npm run clean && tsc -p tsconfig.publish.json -outDir .",
    "postpublish": "npm run clean",
    "dist": "tsc",
    "dev": "npm run clean-build && tsc -p tsconfig.publish.json -outDir build && shx cp package.json build",
    "clean": "npm run clean-src && npm run clean-dirs",
    "clean-src": "shx rm -rf src/**/*.js src/**/*.js.map src/**/*.d.ts *.d.ts *.js *.js.map",
    "clean-build": "shx rm -rf build/**/*",
    "clean-dirs": "shx rm -rf server"
  },
  "author": "xega",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^9.6.23"
  },
  "dependencies": {
    "objio": "^0.0.25"
  }
}
